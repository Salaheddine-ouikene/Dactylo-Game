                        <Font name="Calibri" size="23.0" />
                    </font>
                </Text>
                <Text fx:id="finalScore" fill="GREEN" layoutX="148.0" layoutY="374.0" strokeType="OUTSIDE" strokeWidth="0.0" text="0" textAlignment="CENTER" wrappingWidth="60.30908203125">
                    <font>
                        <Font name="Calibri" size="46.0" />
                    </font>
                </Text>
                <Text fill="GREEN" layoutX="260.0" layoutY="398.0" strokeType="OUTSIDE" strokeWidth="0.0" text="average WordTemp" textAlignment="CENTER" wrappingWidth="80.30908203125">
