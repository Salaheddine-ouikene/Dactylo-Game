                <Label fx:id="TimeText" layoutX="162.0" layoutY="137.0" prefHeight="35.0" prefWidth="276.0" style="-fx-alignment: center;" text="Label" textAlignment="CENTER" textFill="GREEN">
                    <font>
                        <Font name="Calibri" size="40.0" />
                    </font>
                </Label>
                <Button fx:id="playButton" alignment="BOTTOM_LEFT" layoutX="245.0" layoutY="204.0" mnemonicParsing="false" onAction="#PlayNow" prefHeight="35.0" prefWidth="110.0" style="-fx-background-color: #185adb; -fx-cursor: hand;" text="Play" textFill="#ffc947">
